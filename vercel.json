{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/pages/aah",
      "destination": "/pages/aah.html"
    },
    {
      "source": "/pages/are",
      "destination": "/pages/are.html"
    },
    {
      "source": "/pages/apl-etudiant",
      "destination": "/pages/apl-etudiant.html"
    },
    {
      "source": "/pages/asf",
      "destination": "/pages/asf.html"
    },
    {
      "source": "/pages/rsa",
      "destination": "/pages/rsa.html"
    },
    {
      "source": "/pages/prime-activite",
      "destination": "/pages/prime-activite.html"
    },
    {
      "source": "/pages/apl",
      "destination": "/pages/apl.html"
    }
  ],
  "redirects": [
    {
      "source": "/blog/departements/(.*)",
      "destination": "/pages/blog/departements/$1",
      "permanent": true
    },
    {
      "source": "/blog/(.*)",
      "destination": "/pages/blog/$1",
      "permanent": true
    },
    {
      "source": "/:path*",
      "destination": "https://www.lescalculateurs.fr/:path*",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "^lescalculateurs\\.fr$"
        }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "destination": "/$1",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ]
}
